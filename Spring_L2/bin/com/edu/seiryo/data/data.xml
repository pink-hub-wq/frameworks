<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="data" class="com.edu.seiryo.data.Data">
	<property name="array">
		<array>
			<value>张三array</value>
			<value>李四array</value>
			<value>王五array</value>
		</array>
	</property>
	<property name="list">
		<list>
			<value>张三list</value>
			<value>李四list</value>
			<value>王五list</value>
		</list>
	</property>
	<property name="set">
		<set>
			<value>张三set</value>
			<value>李四set</value>
			<value>王五set</value>
		</set>
	</property>
	<property name="map">
		<map>
			<entry>
				<key>
					<value>1</value>
				</key>
				<value>张三map</value>
			</entry>
			<entry key="2" value="李四map"></entry>
			<entry key="3" value="王五map"></entry>
		</map>
	</property>
	<property name="properties">
		<props>
			<prop key="1">张三properties</prop>
			<prop key="2">李四properties</prop>
			<prop key="3">王五properties</prop>
		</props>
	</property>
</bean>
</beans>